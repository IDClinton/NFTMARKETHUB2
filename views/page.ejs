<script>
    var elements = document.querySelectorAll('.values');
    var sum = 0;
    elements.forEach(function(element) {
        var valueText = element.innerText.split('$')[1].trim();
        var value = parseInt(valueText);
        if (!isNaN(value)) {
            sum += value;
        } else {
            console.error('Failed to parse value:', valueText);
        }
    });

    // Log the calculated sum
    console.log('Calculated sum:', sum);

    // Store the sum in localStorage
    localStorage.setItem('sum', sum);
    console.log('Sum stored in localStorage:', sum);
</script>




